{
  "name": "vue3-styled-component",
  "version": "2.0.0",
  "description": "Visual primitives for the component age. A simple port of styled-components ðŸ’… for Vue",
  "author": "Ofumbi Stephen",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "lint": "vue-cli-service lint",
    "build:dist": "rollup -c && rollup -c --environment PRODUCTION",
    "build:lib": "babel --out-dir lib src",
    "build:watch": "npm run build:lib -- --watch",
    "dev": "node example/devServer.js",
    "lint-staged": "lint-staged",
    "prebuild:dist": "rm -rf dist/*",
    "prebuild:lib": "rm -rf lib/*",
    "prebuild:umd": "rm -rf dist/*",
    "prepublish": "npm run build",
    "test": "mocha \"./src/**/*.test.js\" --require @babel/register --require ./mocha-bootstrap --timeout 5000 -R spec --ui bdd",
    "test:watch": "npm run test -- --watch"
  },
  "main": "lib/index.js",
  "module": "dist/vue-styled-components.es.js",
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "core-js": "^3.8.3",
    "inline-style-prefixer": "^6.0.1",
    "lodash.isplainobject": "^4.0.6",
    "lodash.zipobject": "^4.1.3",
    "stylis": "^4.1.1",
    "uninstall": "^0.0.0",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-external-helpers": "^7.17.12",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
