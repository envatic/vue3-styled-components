    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "expect": "^28.1.0",
    "express": "^4.18.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^13.0.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "node-watch": "^0.7.3",
    "pre-commit": "^1.2.2",
    "rollup": "^2.52.8",
